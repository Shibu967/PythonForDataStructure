    "def bubbleSort(a):\n",
    "    for i in range(len(a)):\n",
    "        for j in range(len(a)-i-1):\n",
    "            if a[j]>a[j+1]:\n",
    "                a[j],a[j+1]=a[j+1],a[j]\n",
    "    return a\n",
    "l=[67,4,5,2,178,90]\n",
    "print(bubbleSort(l))"
   ]
